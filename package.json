{
  "name": "contentful-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "sops exec-env contentful-nuxt-development.sops.yaml 'nuxt-ts'",
    "build": "sops exec-env contentful-nuxt-development.sops.yaml 'nuxt-ts build'",
    "start": "sops exec-env contentful-nuxt-development.sops.yaml 'nuxt-ts start'",
    "generate": "sops exec-env contentful-nuxt-development.sops.yaml 'nuxt-ts generate'",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.1",
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/pwa": "^3.3.5",
    "@vue/composition-api": "^1.0.0-rc.1",
    "contentful": "^8.1.7",
    "core-js": "^3.8.3",
    "nuxt": "^2.14.12"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.12",
    "@nuxt/typescript-build": "^2.0.4",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/tailwindcss": "^3.4.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "prettier": "^2.2.1"
  }
}
